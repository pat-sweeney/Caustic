<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\any.png" />
    <None Remove="Resources\bmp.png" />
    <None Remove="Resources\Collapsed.png" />
    <None Remove="Resources\disk.png" />
    <None Remove="Resources\Expanded.png" />
    <None Remove="Resources\folder.png" />
    <None Remove="Resources\jpeg.png" />
    <None Remove="Resources\png.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\any.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
